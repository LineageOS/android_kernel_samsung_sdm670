#include "sdm710-sec-gts4lv-audio-pinctrl.dtsi"

&soc {

};

&tlmm{

	       sdc1_data_on: sdc1_data_on {
				config {
				   pins = "sdc1_data";
				   bias-pull-up;           /* pull up */
				   drive-strength = <12>;  /* 12 MA */
				};
			     };

	       sdc1_data_off: sdc1_data_off {
				 config {
				    pins = "sdc1_data";
				    bias-pull-up;           /* pull up */
				    drive-strength = <2>;   /* 2 MA */
				 };
			      };
		sdc2_cd_on: cd_on {
			mux {
				pins = "gpio3";
				function = "gpio";
			};

			config {
				pins = "gpio3";
				drive-strength = <2>;
				bias-disable;
			};
		};
		
		cam_sensor_eeprom_default: cam_sensor_eeprom_default {
			mux {
				pins = "gpio27", "gpio28";
				function = "gpio";
			};

			config{
				pins = "gpio27", "gpio28";
				bias-pull-down;
				drive-strength = <4>;
				output-low;
			};
		};

		cam_sensor_eeprom_sleep: cam_sensor_eeprom_sleep {
			mux {
				pins = "gpio27", "gpio28";
				function = "gpio";
			};

			config {
				pins = "gpio27", "gpio28";
				bias-pull-down;
				drive-strength = <4>;
				output-low;
			};
		};
		
		
		sdc2_cd_off: cd_off {
			mux {
				pins = "gpio3";
				function = "gpio";
			};

			config {
				pins = "gpio3";
				drive-strength = <2>;
				bias-disable;
			};
		};
	cam_sensor_mclk_0_active: cam_sensor_mclk_0_active {
		/* REAR MCLK */
		mux {
			pins = "gpio13";
			function = "cam_mclk";
		};
		config {
			pins = "gpio13";
			bias-disable; /* No PULL */
			drive-strength = <8>; /* 8 MA */
		};
	};

    cam_sensor_mclk_0_suspend: cam_sensor_mclk_0_suspend {
		/* REAR MCLK */
		mux {
			pins = "gpio13";
			function = "cam_mclk";
		};
		config {
			pins = "gpio13";
			bias-disable; /* PULL DOWN */
			drive-strength = <8>; /* 8 MA */
			output-low;
		};
	};
