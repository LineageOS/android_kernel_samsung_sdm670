				bias-disable;
			};
		};
	};

	etspi_spi_pins: etspi_spi_pins {
		etspi_spi_active: etspi_spi_active {
			mux {
				pins = "gpio81", "gpio82", "gpio83",
							"gpio84";
				function = "qup15";
			};

			config {
				pins = "gpio81", "gpio82", "gpio83",
							"gpio84";
				drive-strength = <6>;
				bias-disable;
			};
		};

		etspi_spi_sleep: etspi_spi_sleep {
			mux {
				pins = "gpio81", "gpio82", "gpio83",
							"gpio84";
				function = "gpio";
			};

			config {
				pins = "gpio81", "gpio82", "gpio83",
							"gpio84";
				drive-strength = <6>;
				bias-pull-down;
				input-enable;
			};
		};
	};

	pinctrl@03400000 {
		sensor_ldo_default: sensor_ldo_default {
			mux {
				pins = "gpio29";
				function = "gpio";
			};

			config {
				pins = "gpio29";
				bias-disable; /* No PULL */
				drive-strength = <2>; /* 2 MA */
				output-high;
			};
		};

		sensor_ldo_active: sensor_ldo_active {
			mux {
				pins = "gpio29";
				function = "gpio";
			};

			config {
				pins = "gpio29";
				bias-disable; /* No PULL */
				drive-strength = <2>; /* 2 MA */
				output-high;
			};
		};

		grip_i2c {
			grip_i2c_active: grip_i2c_active {
				grip_i2c_active {
					pins = "gpio91", "gpio92";
					bias-disable;
				};
			};
			grip_i2c_suspend: grip_i2c_suspend {
				grip_i2c_suspend {
					pins = "gpio91", "gpio92";
					bias-disable;
				};
			};
		};
		
		wifi_grip_i2c {
			wifi_grip_i2c_active: wifi_grip_i2c_active {
				wifi_grip_i2c_active {
					pins = "gpio8", "gpio9";
					bias-disable;
				};
			};
			wifi_grip_i2c_suspend: wifi_grip_i2c_suspend {
				wifi_grip_i2c_suspend {
					pins = "gpio8", "gpio9";
					bias-disable;
				};
			};
		};
		
		grip_int_active: grip_int_active {
			pins = "gpio121";
			function = "normal";
			power-source = <0>;
			input-enable;
			bias-disable;
		};
	
		grip_int_suspend: grip_int_suspend {
			pins = "gpio121";
			function = "normal";
			power-source = <0>;
			input-enable;
			bias-disable;
		};
	};
