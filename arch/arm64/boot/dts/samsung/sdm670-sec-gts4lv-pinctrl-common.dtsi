	fuel_irq: fuel_irq {
		config {
			pins = "gpio24";
			bias-disable;         /* No PULL */
		};
	};

	sm5705_fg_i2c {
		sm5705_fg_i2c_active: sm5705_fg_i2c_active {
			sm5705_fg_i2c {
				pins = "gpio95", "gpio96";
				drive-strength = <2>;
				function = "gpio";
				bias-disable;
				input-enable;
			};
		};
	};
	
	charger_int_pin {
		//qcom,pins = <&gp 69>;
		qcom,num-grp-pins = <1>;
		qcom,pin-func = <0>;
		label = "charger_int_gpio";
		charger_int_active: charger_int_active {
			mux {
				pins = "gpio22";
				function = "gpio";				
			};
			config {
				pins = "gpio22";
				drive-strength = <2>;
				bias-disable;         /* No PULL */
			};
		};
	};
	
 	charger_en_pin {
 		//qcom,pins = <&gp 63>;
		qcom,num-grp-pins = <1>;
		qcom,pin-func = <0>;
		label = "charger_en_gpio";
		charger_en_active: charger_en_active {
			mux {
				pins = "gpio134";
				function = "gpio";				
 			};
			config {
				pins = "gpio134";
				drive-strength = <2>;
				bias-disable;         /* No PULL */
				output-high;
			};
		};
	};

	hall_default: hall_default {
		mux {
			pins = "gpio124";
			function = "gpio";
		};
		config {
			pins = "gpio124";
			drive-strength = <2>;
			bias-disable;
		};
	};

