&qupv3_se12_2uart_pins {
	debug_uart_active: debug_uart_active {
		mux {
			pins = "gpio51", "gpio52";
			function = "qup12";
		};

		config {
			pins = "gpio51", "gpio52";
			drive-strength = <2>;
			output-high;
			bias-pull-up;
		};
	};

	debug_uart_sleep: debug_uart_sleep {
		mux {
			pins = "gpio51", "gpio52";
			function = "gpio";
		};

		config {
			pins = "gpio51", "gpio52";
			drive-strength = <2>;
			bias-pull-up;
			input-enable;
		};
	};
};

&pm660l_gpios {
		wifi_grip_int_active: wifi_grip_int_active {
			pins = "gpio5";
			function = "normal";
			power-source = <0>;
			input-enable;
			bias-disable;
		};
	
		wifi_grip_int_suspend: wifi_grip_int_suspend {
			pins = "gpio5";
			function = "normal";
			power-source = <0>;
			input-enable;
			bias-disable;
		};

		detect_conn_pm_setting: detect_conn_pm_setting {
			pins = "gpio8"; /* UB_CONNECT*/
			function = "normal";
			drive-strength = <0>;
			power-source = <0>;
			qcom,pull-up-strength = <1>;
			bias-pull-up; /* PULL UP*/
			input-enable;
		};

};
&pm660_gpios {
		rome_cable_default: rome_cable_default {
			pins = "gpio6";
			bias-disable;
		};
};